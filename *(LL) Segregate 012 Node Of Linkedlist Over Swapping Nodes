   public static ListNode segregate012(ListNode head) {
        if(head == null || head.next == null){
            return head;
        }
        ListNode zero = new ListNode(-1);
        ListNode temp0 = zero;
        ListNode one = new ListNode(-1);
        ListNode temp1 = one;
        ListNode two = new ListNode(-1);
        ListNode temp2 = two;
        ListNode temp = head;
        
        while(temp!=null){
            ListNode nn = new ListNode(temp.val);
            temp = temp.next;
            if(nn.val == 0){
                temp0.next = nn;
                temp0 = nn;
                
            }else if(nn.val == 1){
                temp1.next = nn;
                temp1 = nn;
                
            }else{
                temp2.next = nn;
                temp2 =nn;
                
            }
        }
        
        //temp0.next = null;
        //temp1.next = null;
        temp2.next = null;
        temp1.next = two.next;
        temp0.next = one.next;
        
        
        return zero.next;
        
        
    }
